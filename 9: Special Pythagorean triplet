import math
add = 0
goal = 1000 #The number you want it to add up too
a = 1
b = 1
c = 1
maybe = 1000 # The amount of numbers it goes up too
finish = 'false'
for i in range(1,maybe+1):
  for j in range(1,maybe+1):
    for k in range(1,maybe+1):
      if math.sqrt(i*i+j*j) == k:
        print(str(i)+' '+str(j)+' '+str(k))
        if i+j+k == goal:
          print('finish')
print(i*j*k)
