num = 1
multis = 0
amount = 1
total = 2
wanted = 10001 #Number of primes wanted
print(2)
while num<2000000:
  multis = 0
  for i in range(1,num):
    if num%i == 0:
      multis += 1
  if multis == 1:
    amount += 1
    print(num)
    total = total + num
  if amount == wanted:
    break
  num += 2
print('finish')
print(total)
